<% include partials/head.ejs %>
<main>
    <form id="login_form" class="opr_bruger" action="/auth/opret" method="POST" autocomplete="off">
      
    
        <div class="inputGroup">
            <h1><%= title %></h1>
           
            <input type="text" name="username" id="username" placeholder="Brugernavn" required> 
            <input type="email" name="email" placeholder="Email" required>
            <input type="password" name="passphrase" id="passphrase" placeholder="Adgangskode" required>
            <!-- <input type="password" name="passphrase" id="passphrase" placeholder="Gentag adgangskode" required> -->
            <button type="submit">OPRET</button>
        </div>
    </form>
    
</main>
